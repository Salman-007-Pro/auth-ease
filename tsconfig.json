{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "strict": true,
        "lib": ["es2015", "dom"],
        "esModuleInterop": true,
        "paths": {
            "@/*": ["./src/*"]
        }
    },
    "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts", "plugins/withGradleHeapFix.js"]
}
